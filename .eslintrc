{
  "env": {
    "es2023": true,
    "node"  : true
  },
  "extends"  : "eslint:recommended",
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files"        : ["*.msj"],
      "parserOptions": {
        "sourceType": "module"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType" : "module"
  },
  "rules": {
    "semi": ["error", "always"]
  }
}
